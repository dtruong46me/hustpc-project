import numpy as np

cov_matrix = np.array([[1.3333, -0.3333, -1.0000],
                       [-0.3333, 0.3333, 0.3333],
                       [-1.0000, 0.3333, 3.3667]])

eigenvalues, eigenvectors = np.linalg.eig(cov_matrix)

print("Eigenvalues:")
print(eigenvalues)
print("\nEigenvectors:")
print(eigenvectors)

# Lengths of the axes
axis_lengths = np.sqrt(eigenvalues)

print("\nLengths of the Axes:")
print(axis_lengths)